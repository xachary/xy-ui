{
  "name": "@xachary/xy-ui",
  "version": "0.1.15",
  "description": "vue ui lib",
  "main": "dist/xy-ui.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --hot --host 0.0.0.0 | yarn format",
    "start": "./node_modules/spa-http-server/bin/http-server -p 6666 --push-state",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "stylelint": "onchange 'src/**/*.css' 'src/**/*.scss' 'src/**/*.vue' 'src/**/*.html' -- stylelint --fix {{changed}}",
    "prettier": "onchange 'src/**/*.js' 'src/**/*.json' 'src/**/*.vue' 'src/**/*.html' -- prettier --write {{changed}}",
    "eslint": "onchange 'src/**/*.js' 'src/**/*.json' 'src/**/*.vue' 'src/**/*.html' -- eslint {{changed}}",
    "format": "yarn run stylelint | yarn run prettier"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xachary/xy-ui.git"
  },
  "keywords": [
    "vue",
    "ui",
    "component",
    "directive",
    "filter"
  ],
  "author": "xachary",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/xachary/xy-ui/issues"
  },
  "homepage": "https://github.com/xachary/xy-ui#readme",
  "dependencies": {
    "photoswipe": "^4.1.3",
    "vue": "^2.6.10",
    "vue-lazyload": "^1.2.6",
    "vue-router": "^3.0.1",
    "vux": "^2.9.4"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-3": "^6.24.1",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^6.0.3",
    "css-loader": "^0.28.11",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-vue": "^6.2.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^4.3.0",
    "node-sass": "^4.13.1",
    "onchange": "^6.1.0",
    "prettier": "^2.0.2",
    "sass-loader": "^6.0.6",
    "spa-http-server": "^0.9.0",
    "stylelint": "^13.2.1",
    "stylelint-order": "^4.0.0",
    "stylelint-scss": "^3.16.0",
    "url-loader": "^2.3.0",
    "vue-loader": "^13.0.5",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  }
}
